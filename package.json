{
  "name": "samyrai-resume",
  "version": "1.0.0",
  "description": "Modern resume website built with Hugo and Tailwind CSS",
  "scripts": {
    "dev": "./node_modules/.bin/hugo server --config hugo.toml --buildDrafts --buildFuture",
    "build": "hugo --minify",
    "build:production": "hugo --minify --environment production"
  },
  "devDependencies": {
    "autoprefixer": "^10.4.19",
    "hugo-extended": "^0.88.1",
    "postcss-cli": "^11.0.0",
    "tailwindcss": "^4.1.12"
  },
  "keywords": [
    "hugo",
    "resume",
    "cv",
    "portfolio",
    "tailwindcss"
  ],
  "author": "Damir Mukimov",
  "license": "MIT"
}